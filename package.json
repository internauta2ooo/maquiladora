        "eslint": "^7.18.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-vue": "^7.5.0",
        "eslint-webpack-plugin": "^2.4.3",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.9",
        "laravel-mix-eslint-config": "^0.1.7",
        "lodash": "^4.17.19",
        "popper.js": "^1.12",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.0",
