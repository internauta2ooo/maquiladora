        "vue-template-compiler": "^2.6.12",
        "webpack-dev-server": "^3.11.1"
    },
    "dependencies": {
        "bootstrap-vue": "^2.20.1",
        "moment": "^2.29.1",
        "object-path": "^0.11.5",
        "signature_pad": "^3.0.0-beta.4",
